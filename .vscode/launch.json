      "cwd": "c:/Users/ARMIDE Informatique/Desktop/Projet Informatique/Programation/Projet C/trying",
      "program": "c:/Users/ARMIDE Informatique/Desktop/Projet Informatique/Programation/Projet C/trying/build/Debug/outDebug",
